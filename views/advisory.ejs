<%- include("partials/header"); -%>

  <div class="container">
    <div class="heading text-center">Advisory Member</div>

  <%advisorys.forEach(function(advisory,index){  %>

      <div class="row life">
        <div class="col-sm-3">
          <% if(advisory.image){%>
            <img class="up-img"src="/uploads/<%=advisory.image %>" alt="profile picture">
          <% } %>

        </div>
        <div class="data1 col-sm-9">
          <p><strong class="h3">Name: </strong><%=advisory.name %></p>
          <p><strong class="h3">Membership_id: </strong><span><u>AM </u><%=index+1%></span></p>
          <p><strong class="h3">Current Position & Affiliation: </strong><%=advisory.position %></p>
          <p><strong class="h3">Masters Research Field & Institution : </strong><%=advisory.masters %></p>
          <p><strong class="h3">PhD Research Field & Institution: </strong><%=advisory.phd %></p>
          <p><strong class="h3">Areas of Specialization: </strong><%=advisory.areaOfSpecialization %></p>
          <p><strong class="h3">Current Research Interest: </strong><%=advisory.current %></p>
          <p><strong class="h3">Contact Address & Mobile : </strong><%=advisory.contact %></p>
          <p><strong class="h3">Email: </strong><%=advisory.mail %></p>
        </div>

      </div>


    <hr>
  <%});%>

</div>
<canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>

<%- include("partials/footer"); -%>
