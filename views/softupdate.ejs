<%-include('partials/adminheader'); -%>

<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Society Update Link</h1>

  </div>




</main>
</div>
</div>
<div class="container">
  <div class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
    <form class="" action="/softupdate" method="post"enctype="multipart/form-data">

      <div class="form-group">
        <label>software name:</label>
        <input class="form-control"type="text" name="title" value="" autocomplete="off">
        <label>Download link:</label>
        <input class="form-control"type="file" name="file" value="">


      </div>


      <button class="btn btn-dark mt-4" type="submit" name="button">Publish</button>
    </form>
    <div class="container mt-5 mr-5">
      <table class="table" border="1">
        <thead>
          <tr>

            <th>software name</th>

            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
          <%softs.forEach(function(soft){%>
          <tr>

            <td><%=soft.title%></td>


            <td><a class="delete" href="/sfdelete/<%=soft._id%>">Delete</a></td>
          </tr>
            <%});%>
        </tbody>
      </table>
    </div>
  </div>

</div>


<canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>


<%-include('partials/adminfooter'); -%>
