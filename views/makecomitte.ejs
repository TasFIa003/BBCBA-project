<%-include('partials/adminheader'); -%>

<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Add Executive Member</h1>

  </div>




  </main>
</div>
</div>
<div class="container">
  <div class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
    <form class="" action="/makecomitte" method="post">
      <div class="form-group">
          <label>Committe title:</label>
          <input class="form-control" type="text" name="year" autocomplete="off">
          <label>Position title</label>
          <input class="form-control" type="text" name="position" autocomplete="off">
          <label>Name:</label>
          <input class="form-control" type="text" name="name"autocomplete="off">
          <label>Institution:</label>
          <input class="form-control" type="text" name="insti"autocomplete="off">

        </div>
        <button class="btn btn-dark mt-4" type="submit" name="button">Publish</button>
        </form>
        <div class="container mt-5 mr-5">
          <table class="table" border="1">
            <thead>
              <tr>
                <th>Title</th>
                <th>Position title</th>
                <th>Name</th>
                <th>Institution</th>
                <th>Update</th>
                <th>Delete</th>
              </tr>
            </thead>
            <tbody>
              <%coms.forEach(function(com){%>
              <tr>
                <td><%=com.year%></td>
                <td><%=com.position%></td>
                <td><%=com.name%></td>
                <td><%=com.insti%></td>
                <td><a class="edit" href="/comedit/<%=com._id%>">Update </a></td>
                <td><a class="delete" href="/comdelete/<%=com._id%>">Delete</a></td>
              </tr>
                <%})%>
            </tbody>
          </table>
        </div>
        </div>

  </div>



<canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>


<%-include('partials/adminfooter'); -%>
