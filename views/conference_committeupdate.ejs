<%-include('partials/adminheader'); -%>

<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
  <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Conference Committe page update </h1>

  </div>




</main>

<div class="container">
  <div class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
    <form class="" action="/conference_committeupdate" method="post" enctype="multipart/form-data">

      <div class="form-group">

        <label>Details:</label>
        <textarea class="form-control" id="editor" name="name" rows="8" cols="80"></textarea>
        <label>Sub Committe:</label>
        <input class="form-control" type="text" name="title" value="">
      </div>


      <button class="btn btn-dark mt-4" type="submit" name="button">Publish</button>
    </form>
    <div class="container mt-5 mr-5">
      <table class="table" border="1">
        <thead>
          <tr>

            <th>Details</th>
            <th>sub Committee</th>
            <th>Update</th>
            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
          <%cocos.forEach(function(coco){%>
          <tr>

            <td><%-coco.name%></td>
            <td><%=coco.title%></td>
            <td><a class="edit" href="/cocoedit/<%=coco._id%>">Update</a></td>
            <td><a class="delete" href="/cocodelete/<%=coco._id%>">Delete</a></td>
          </tr>
            <%});%>
        </tbody>
      </table>
    </div>
  </div>

</div>


<canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>


<%-include('partials/adminfooter'); -%>
